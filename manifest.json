{
  "manifest_version": 3,
  "name": "Fidelity Transaction Scraper",
  "version": "1.0.0",
  "description": "Scrapes and parses transaction data from Fidelity cash management accounts",
  "browser_specific_settings": {
    "gecko": {
      "id": "fidelity2ynab@example.com",
      "strict_min_version": "109.0"
    }
  },
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "host_permissions": [
    "https://digital.fidelity.com/*",
    "https://api.ynab.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://digital.fidelity.com/*"],
      "js": ["browser-polyfill.min.js", "lib/banks/fidelity/scraper.js", "content.js"]
    }
  ]
}
