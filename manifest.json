{
  "manifest_version": 3,
  "name": "Fidelity Transaction Scraper",
  "version": "1.0.0",
  "description": "Scrapes and parses transaction data from Fidelity cash management accounts",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "host_permissions": [
    "https://digital.fidelity.com/*",
    "https://api.ynab.com/*"
  ]
}
